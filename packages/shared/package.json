  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "ioredis": "^5.3.2",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
